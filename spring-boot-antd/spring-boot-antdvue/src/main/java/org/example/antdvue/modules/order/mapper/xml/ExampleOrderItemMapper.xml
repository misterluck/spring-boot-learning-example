<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.antdvue.modules.order.mapper.ExampleOrderItemMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  example_order_item 
		WHERE
			 order_fk_id = #{mainId} 	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.example.antdvue.modules.order.entity.ExampleOrderItem">
		SELECT * 
		FROM  example_order_item
		WHERE
			 order_fk_id = #{mainId} 	</select>
</mapper>
